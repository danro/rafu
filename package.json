{
  "name": "rafu",
  "version": "1.0.0",
  "description": "Minimal requestAnimationFrame shim and utility",
  "author": "Dan Rogers @danro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danro/rafu"
  },
  "keywords": [
    "animation",
    "frame",
    "throttle"
  ],
  "scripts": {
    "start": "webpack-dev-server --port 4000",
    "test": "npm start && open http://localhost:4000/tests.html"
  },
  "main": "rafu.js",
  "dependencies": {},
  "devDependencies": {
    "chai": "^2",
    "mocha": "^2",
    "sinon-chai": "^2.7.0",
    "webpack": "^1",
    "webpack-dev-server": "^1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "strict": 0,
      "indent": [
        2,
        2
      ],
      "no-extra-strict": 0,
      "quotes": [
        2,
        "single"
      ],
      "curly": [
        2,
        "multi-line"
      ],
      "consistent-return": 0,
      "no-unused-expressions": 0,
      "no-mixed-requires": 0,
      "no-underscore-dangle": 0,
      "no-comma-dangle": 0,
      "comma-dangle": "never",
      "no-use-before-define": [
        1,
        "nofunc"
      ],
      "no-empty": 0,
      "new-parens": 0,
      "no-unused-vars": 0,
      "no-loop-func": 0,
      "new-cap": 0
    },
    "globals": {
      "sinon": false,
      "describe": false,
      "it": false
    }
  }
}
